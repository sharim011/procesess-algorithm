   "source": [
    "import array\n",
    "from array import *\n",
    "\n",
    "def insertion_sort(array):\n",
    "    for i in range(1,len(arr)):\n",
    "        j=i\n",
    "        while arr[j-1]>arr[j]and j>0:\n",
    "            arr[j-1],arr[j]=arr[j],arr[j-1]\n",
    "            j=j-1\n",
    "arr=array('i',[2,6,5,1,3,4])\n",
    "insertion_sort(arr)\n",
    "print(arr)"
   ]
